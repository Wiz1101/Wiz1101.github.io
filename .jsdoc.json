{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src",
      "public/js"
    ],
    "exclude": [ 
      "build",
      "dist",
      "node_modules"
     ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./build/jsdoc/",
    "recurse": true
  }
}
